<div class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-white border-bottom box-shadow">
      <h5 class="my-0 mr-md-auto font-weight-normal">Guitar Zone</h5>
      	<nav class="my-2 my-md-0 mr-md-3">       
		    <%=link_to :Home, root_path,class:"p-2 text-dark"%>
			<%if current_user%>
					<%if logged_in?(:owner)%>
						<%=link_to :Guitars,guitars_path,class:"p-2 text-dark"%>
						<%=link_to :Accessories,accessories_path,class:"p-2 text-dark"%>
						<%=link_to :Invoices,all_orders_path,class:"p-2 text-dark"%>
					<%else%>
						<span class="p-2 text-dark" id="cart">Items in Cart: <%=session[:cart].length%></span>
						<%=link_to :Checkout,checkout_path,class:"p-2 text-dark"%>
						<%=link_to "My Orders",my_orders_path,class:"p-2 text-dark"%>
					<%end%>
		</nav>
					<%=link_to :Logout, destroy_user_session_path , method: :delete,class:"btn btn-outline-danger"%>
			<%else%>
					<%=link_to :Login, new_user_session_path,class:"btn btn-outline-success"%>
			<%end%>      
</div>



