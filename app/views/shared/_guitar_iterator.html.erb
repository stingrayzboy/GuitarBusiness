
	<table class="table">
		  <thead>
		    <tr>
		   		<th scope="col">Item</th>
				<th scope="col">Model</th>
				<th scope="col">Brand</th>
				<th scope="col">Serial</th>
		    </tr>
		  </thead>
		  <tbody>
		  	<%guitar.each_with_index do |p,idx|%>
		  		<tr>
			      <th scope="row"><%=idx+1%></th>
			      <td><%=p.model%></td>
			      <td><%=p.brand%></td>
			      <td><%=p.serial.upcase%></td>
			    </tr>

		  	<%end%>
		  </tbody>
		</table>